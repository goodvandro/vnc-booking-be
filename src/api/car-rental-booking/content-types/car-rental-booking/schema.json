{
  "kind": "collectionType",
  "collectionName": "car_rental_bookings",
  "info": {
    "singularName": "car-rental-booking",
    "pluralName": "car-rental-bookings",
    "displayName": "Car Rental Booking"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bookingId": {
      "type": "uid",
      "required": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "driverLicense": {
      "type": "string",
      "required": false
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "totalPrice": {
      "type": "decimal",
      "required": true
    },
    "bookingStatus": {
      "type": "enumeration",
      "required": true,
      "default": "pending",
      "enum": [
        "pending",
        "confirmed",
        "cancelled",
        "completed"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "car": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::car.car"
    }
  }
}
