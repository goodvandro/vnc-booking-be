{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "carId": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "seats": {
      "type": "integer",
      "required": true,
      "default": 4
    },
    "transmission": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
