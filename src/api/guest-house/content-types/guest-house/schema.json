{
  "kind": "collectionType",
  "collectionName": "guest_houses",
  "info": {
    "singularName": "guest-house",
    "pluralName": "guest-houses",
    "displayName": "Guest House"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "guestHouseId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "default": 4
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
